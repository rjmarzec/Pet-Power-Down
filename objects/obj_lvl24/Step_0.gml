		/// @DnDSaveInfo : "spriteind" "4837db0a-7b66-4554-9eab-df4a7c7895ab"
		sprite_index = spr_lvl24s;
		image_index += 0;
	
		/// @DnDAction : YoYo Games.Common.If_Variable
		/// @DnDVersion : 1
		/// @DnDHash : 53B2AE61
		/// @DnDParent : 5A028FD0
		/// @DnDArgument : "var" "global.playedsound24"
		if(global.playedsound24 == 0)
		{
			/// @DnDAction : YoYo Games.Audio.Play_Audio
			/// @DnDVersion : 1
			/// @DnDHash : 0B530D19
